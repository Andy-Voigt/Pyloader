{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background/chromereload.js",
      "scripts/background/clickandload.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "background",
    "clipboardRead",
    "contentSettings",
    "notifications",
    "storage",
    "webNavigation",
    "webRequest",
    "*://*.google.com/",
    "webRequestBlocking"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "pyloader",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "scripts/jdcheck.js"
  ]
}
